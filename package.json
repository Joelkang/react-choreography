{
  "name": "react-choreography",
  "version": "0.6.0",
  "description": "Simple animations / transition components in React",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-choreography"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "keywords": [
    "react-component",
    "transition",
    "animation",
    "css-transition-group",
    "staggered"
  ],
  "scripts": {
    "dev:client": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.3",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "file-loader": "0.11.1",
    "flow-bin": "^0.46.0",
    "nodemon": "^1.11.0",
    "react": "next",
    "react-dom": "next",
    "react-transition-group": "next",
    "source-map-support": "^0.4.15",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "react": "next",
    "react-dom": "next",
    "react-transition-group": "next"
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
