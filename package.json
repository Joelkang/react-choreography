{
  "name": "react-choreography",
  "version": "0.8.5",
  "description": "Simple animations / transition components in React",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Setsun/react-choreography"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^5.0.0"
  },
  "keywords": [
    "react-component",
    "transition",
    "animation",
    "css-transition-group",
    "staggered"
  ],
  "scripts": {
    "dev:client":
      "cross-env NODE_ENV=development webpack --config webpack.config.js --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "precommit": "lint-staged",
    "eslint-prettier-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@storybook/react": "^3.2.10",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^7.0.0-beta.1",
    "babel-core": "^7.0.0-beta.1",
    "babel-eslint": "^8.0.0-alpha.17",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.1",
    "babel-preset-env": "^2.0.0-beta.1",
    "babel-preset-react": "^7.0.0-beta.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "^4.7.2",
    "eslint-config-eslint": "^4.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.5",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.2",
    "nodemon": "^1.12.1",
    "office-ui-fabric-react": "^5.12.0",
    "prettier": "^1.7.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-transition-group": "^2.2.0",
    "source-map-support": "^0.5.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-transition-group": "^2.2.0"
  },
  "babel": {
    "presets": [
      "react",
      "stage-0",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": ["transform-optional-chaining"]
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "eslintConfig": {
    "extends": ["babel-eslint", "eslint", "prettier"],
    "plugins": ["import", "react", "prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "printWidth": 80
        }
      ]
    }
  }
}
